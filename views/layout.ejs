<body>

    <%- include('./_header.ejs') %>


    <main>
        <%- body %>
    </main>

    <%- script %>
        <script>
        <% if (flash.success && flash.success.length > 0) { %>

                new Noty({
                    theme: 'relax',
                    text: '<%=flash.success%>',
                    type: 'success',
                    layout: 'topRight',
                    timeout: 1500
                }).show();
    <% } %>

    <% if (flash.error && flash.error.length > 0) { %>
                new Noty({
                    theme: 'relax',
                    text: '<%=flash.error%>',
                    type: 'error',
                    layout: 'topRight',
                    timeout: 1500
                }).show();
    <% } %>

    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
